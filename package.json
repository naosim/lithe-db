{
  "name": "jsondb",
  "version": "1.4.0",
  "description": "JSONファイルに保存する、AIフレンドリーな軽量データベース。",
  "main": "./dist/lithe-db.cjs",
  "module": "./dist/lithe-db.js",
  "types": "./dist/lithe-db.d.ts",
  "exports": {
    ".": {
      "types": "./dist/lithe-db.d.ts",
      "import": "./dist/lithe-db.js",
      "require": "./dist/lithe-db.cjs"
    }
  },
  "bin": {
    "lithe-db": "./dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "node build.js",
    "prepublishOnly": "npm run build",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "devDependencies": {
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "dependencies": {
    "googleapis": "^171.0.0"
  }
}
